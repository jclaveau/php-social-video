{
    "name": "jclaveau/php-social-video",
    "description": "Library to ease handling of videos from social networks.",
    "license": "MIT",
    "keywords": ["youtube", "dailymotion", "vimeo", "facebook", "twitch"],
    "authors": [
        {
            "name": "Jean Claveau",
            "role": "Developer"
        },
        {
            "name": "lingtalfi",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4" : {
            "JClaveau\\SocialVideo\\" : "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jclaveau/php-visibility-violator"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "jclaveau/php-visibility-violator": "dev-master",
        "evert/phpdoc-md" : "~0.2.0",
        "phpdocumentor/phpdocumentor" : "~2.8.0"
    },
    "require": {
        "php": ">=5.5.0"
    }
}
